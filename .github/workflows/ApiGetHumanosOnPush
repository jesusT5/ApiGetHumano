name: ApiGetHumanosOnPush

on: [push]

jobs:
  build:
    runs-on: ubuntu-latest
    steps: 
      - name: Get Code
        uses: actions/checkout@v3
      - name: Setup dotnet
        uses: actions/dotnet@v3
        with: 
          dotnet-version: '5.0.x'
      - name: Install dependencies
        run: dotnet restore
      - name: Build
        run: dotnet build
      - name: Test with the dotnet CLI
        run: dotnet test
          
